page{
  background-color: #fff;
}

.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

button {
  background-color: inherit;
  line-height: inherit;
  padding: 0;
  border: none;
  margin: 0;
  font-size: inherit;
  border-radius: 0;
}

button::after {
  border: none;
}