.header {
  display: flex;
  align-items: center;
  top: 0;
  position: fixed;
  width: 100%;
  z-index: 99999;
}

.header .header-title {
  font-weight: 600;
  margin:0 30rpx;
  display: flex;
  align-items: center;
  width: 100%;
}

.back-black {
  background: url(data:image/png;base64,) no-repeat center center;
}

.back-white {
  background: url(data:image/png;base64,) no-repeat center center;
}

.back {
  margin-right: 6px;
  background-size: 100% 100%;
  width: 36rpx;
  height: 36rpx;
}

.title{
  height: 36rpx;
  line-height: 36rpx;
  font-size: 36rpx;
  flex-grow: 1;
}
